{
  "name": "verihome-e2e-testing",
  "version": "1.0.0",
  "description": "End-to-End Testing Suite for VeriHome Platform",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:headless": "cypress run --headless",
    "test:e2e": "cypress run --spec 'cypress/e2e/**/*.cy.js'",
    "test:websocket": "cypress run --spec 'cypress/e2e/websocket/**/*.cy.js'",
    "test:auth": "cypress run --spec 'cypress/e2e/auth/**/*.cy.js'",
    "test:properties": "cypress run --spec 'cypress/e2e/properties/**/*.cy.js'",
    "test:contracts": "cypress run --spec 'cypress/e2e/contracts/**/*.cy.js'",
    "test:full": "npm run test:auth && npm run test:properties && npm run test:websocket && npm run test:contracts",
    "test:smoke": "cypress run --spec 'cypress/e2e/smoke/**/*.cy.js'",
    "reports": "cypress run --reporter mochawesome --reporter-options reportDir=cypress/reports,overwrite=false,html=false,json=true",
    "merge-reports": "mochawesome-merge cypress/reports/*.json > cypress/reports/merged-report.json",
    "generate-report": "marge cypress/reports/merged-report.json --reportDir cypress/reports --inline"
  },
  "keywords": [
    "cypress",
    "e2e",
    "testing",
    "verihome",
    "websocket",
    "django",
    "react"
  ],
  "author": "VeriHome Development Team",
  "license": "MIT",
  "devDependencies": {
    "@cypress/grep": "^4.0.1",
    "cypress": "^13.17.0",
    "cypress-drag-drop": "^1.1.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-real-events": "^1.11.0",
    "cypress-wait-until": "^3.0.1",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "socket.io-client": "^4.7.4",
    "ws": "^8.16.0"
  }
}
