{
  "name": "inmobiliaria-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "ANALYZE=true npm run build",
    "build:prod": "NODE_ENV=production tsc && vite build",
    "build:staging": "NODE_ENV=staging tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:open": "jest --coverage && open coverage/lcov-report/index.html",
    "test:silent": "jest --silent",
    "test:components": "jest --testPathPatterns=components",
    "test:hooks": "jest --testPathPatterns=hooks",
    "test:services": "jest --testPathPatterns=services",
    "test:pages": "jest --testPathPatterns=pages",
    "test:integration": "jest --testPathPatterns=integration",
    "test:unit": "jest --testPathPatterns='(components|hooks|services|pages)' --testPathIgnorePatterns=integration",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headed": "cypress run --headed",
    "test:component": "cypress run --component",
    "test:full": "npm run test:ci && npm run test:e2e",
    "test:auth": "jest --testNamePattern='auth' --coverage",
    "test:properties": "jest --testNamePattern='propert' --coverage",
    "test:performance": "npm run build:prod && lighthouse http://localhost:4173 --only-categories=performance --chrome-flags='--headless'",
    "optimize": "npm run lint:fix && npm run type-check && npm run test:ci && npm run build:analyze",
    "performance": "npm run build:prod && npm run preview",
    "size-check": "npm run build && du -sh ../staticfiles/frontend/* | sort -hr"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@hello-pangea/dnd": "18.0.1",
    "@mapbox/mapbox-gl-geocoder": "5.0.3",
    "@mui/icons-material": "^5.15.10",
    "@mui/lab": "5.0.0-alpha.170",
    "@mui/material": "^5.15.10",
    "@mui/x-data-grid": "8.5.2",
    "@mui/x-date-pickers": "8.9.0",
    "@paypal/react-paypal-js": "8.8.3",
    "@stripe/react-stripe-js": "3.7.0",
    "@stripe/stripe-js": "7.4.0",
    "@tanstack/react-query": "^5.20.5",
    "@types/react-dropzone": "4.2.2",
    "axios": "^1.6.7",
    "chart.js": "4.5.0",
    "date-fns": "4.1.0",
    "leaflet": "1.9.4",
    "mapbox-gl": "3.13.0",
    "react": "^18.2.0",
    "react-chartjs-2": "5.3.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "14.3.8",
    "react-hook-form": "7.58.1",
    "react-leaflet": "4.2.1",
    "react-router-dom": "^6.22.1",
    "react-swipeable": "7.0.2",
    "react-toastify": "11.0.5",
    "recharts": "2.15.3",
    "xlsx": "0.18.5"
  },
  "devDependencies": {
    "@esbuild/linux-x64": "0.25.5",
    "@rollup/rollup-linux-x64-gnu": "4.44.2",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/leaflet": "1.9.18",
    "@types/mapbox__mapbox-gl-geocoder": "5.0.0",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.5.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "identity-obj-proxy": "3.0.0",
    "jest": "30.0.3",
    "jest-environment-jsdom": "30.0.2",
    "msw": "2.10.2",
    "prettier": "3.6.2",
    "ts-jest": "29.4.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
  }
}
